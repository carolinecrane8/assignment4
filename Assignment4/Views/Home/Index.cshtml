@{
    ViewData["Title"] = "Top Restaurants";
}
@model IEnumerable<string>
<!--This is the home page. The top 5 restaurants will be displayed here.-->
<!DOCTYPE html>
<html>
<head>
    <title>Top Restaurants</title>
    <meta name="viewport" content="width=device-width"/>
</head>
<body>
    <br />
    <div class="row">
        <div class="row" >
            <img src="~/img/img1.png" alt="Chick" style="width:19%">
       
            <img src="~/img/img2.jpg" alt="Azo" style="width:20%">
       
            <img src="~/img/img3.png" alt="cubbys" style="width:20%">
      
            <img src="~/img/img4.png" alt="burger" style="width:20%">
        
            <img src="~/img/img5.png" alt="caferio" style="width:19%">
        </div>
    </div>
    <br />
    

    <div class="text-center">
        <ul class="list-unstyled">
            @foreach (string r in Model)
            {
                <li>@r</li>
                <br />
            }
        </ul>
    </div>
</body>

</html>